{
  "name": "simone-bianco/laravel-rag-chunks",
  "version": "1.0.0",
  "description": "Laravel package that organizes chunks of text for AI operations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {"name": "Simone Bianco"}
  ],
  "require": {
      "php": ">=8.3",
      "illuminate/support": "^12.0",
      "illuminate/validation": "^12.0",
      "illuminate/console": "^12.0",
      "illuminate/cache": "^12.0",
      "spatie/laravel-package-tools": "^1.92",
      "maestroerror/laragent": "*",
      "tpetry/laravel-postgresql-enhanced": "^3.5",
      "simone-bianco/laravel-processes": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "^12.3",
      "orchestra/testbench": "^10.6",
      "pestphp/pest": "^3.8",
      "pestphp/pest-plugin-laravel": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "SimoneBianco\\LaravelRagChunks\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SimoneBianco\\LaravelRagChunks\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SimoneBianco\\LaravelRagChunks\\LaravelRagChunksServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
